@page
@model Ailog.Pages.Logs.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Barang</h1>

<div>
    <h4>Log</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Log.Nama_Barang)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Log.Nama_Barang)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Log.Waktu)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Log.Waktu)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Log.Jenis_Barang)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Log.Jenis_Barang)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Log.Status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Log.Status)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Log.ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
    </form>
</div>
